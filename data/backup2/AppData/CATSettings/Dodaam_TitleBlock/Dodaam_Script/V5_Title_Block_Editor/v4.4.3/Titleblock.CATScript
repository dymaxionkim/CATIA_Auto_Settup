		bStreamAllProperties = false
		
		TBlockPrefix = "TitleBlock_Text"
		TBlockPrefixLen=15
   		Do until basics_stream.atEndOfStream
			zeile = basics_stream.ReadLine
			zeile = MHNReplaceChar(zeile,chr(10),"")
			zeile = MHNReplaceChar(zeile,chr(13),"")
